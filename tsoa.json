{
  "entryFile": "src/controllers.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "spec": {
    "name": "TSOA Starter",
    "outputDirectory": "dist",
    "specVersion": 3,
    "securityDefinitions": {
      "api_key": {
        "type": "apiKey",
        "name": "access_token",
        "in": "header"
      }
    },
    "spec": {
      "info": {
        "x-logo": "/logo.png",
        "x-favicon": "/favicon.ico"
      }
    },
    "specMerging": "deepmerge"
  },
  "routes": {
    "routesDir": "src/generated",
    "authenticationModule": "./src/auth.ts"
  }
}
