{
    "entryFile": "src/app.ts",
    "noImplicitAdditionalProperties": "throw-on-extras",
    "spec": {
        "outputDirectory": "dist",
        "specVersion": 3,
        "securityDefinitions": {
          "api_key": {
            "type": "apiKey",
            "name": "access_token",
            "in": "header"
          }
        }
    },
    "routes": {
        "routesDir": "src/generated",
        "authenticationModule": "./src/auth.ts"
    }
}
